{
  "name": "@butnothero/axios-wrapper",
  "version": "1.0.0",
  "description": "Generic helper class for wrapping axios calls",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ButNotHero/axios-wrapper"
  },
  "author": "butnothero@gmail.com",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "yarn run build:clean && yarn run build:esm && yarn build:cjs",
    "build:clean": "rimraf dist",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^0.21.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "axios": "^0.21.2",
    "lodash": "^4.17.11"
  }
}
